    "lint": "npx solhint \"contracts/**/*.sol\"",
    "slither": "slither ."
  },
  "dependencies": {},
  "devDependencies": {
    "hardhat": "^2.1.2",
    "solhint": "^3.3.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
